<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Officesupplies" table="officesupplies">
    <id name="officesuppliesId" type="integer" column="officesupplies_id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="officesupplies_officesupplies_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="type" type="string" column="type" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="purchased" type="date" column="purchased" nullable="true"/>
    <field name="price" type="decimal" column="price" precision="10" scale="0" nullable="true"/>
  </entity>
</doctrine-mapping>
