<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Permissions" table="permissions">
    <id name="permissionId" type="integer" column="permission_id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="permissions_permission_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="permission" type="string" column="permission" length="125" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
